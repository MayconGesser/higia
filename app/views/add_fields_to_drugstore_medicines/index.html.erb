<p id="notice"><%= notice %></p>

<h1>Add Fields To Drugstore Medicines</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_fields_to_drugstore_medicines.each do |add_fields_to_drugstore_medicine| %>
      <tr>
        <td><%= add_fields_to_drugstore_medicine.street %></td>
        <td><%= add_fields_to_drugstore_medicine.city %></td>
        <td><%= add_fields_to_drugstore_medicine.state %></td>
        <td><%= add_fields_to_drugstore_medicine.number %></td>
        <td><%= link_to 'Show', add_fields_to_drugstore_medicine %></td>
        <td><%= link_to 'Edit', edit_add_fields_to_drugstore_medicine_path(add_fields_to_drugstore_medicine) %></td>
        <td><%= link_to 'Destroy', add_fields_to_drugstore_medicine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

